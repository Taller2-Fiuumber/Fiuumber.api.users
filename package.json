{
    "name": "back-template",
    "version": "1.0.0",
    "description": "",
    "main": "index.ts",
    "scripts": {
        "test": "jest --",
        "coverage": "jest --collectCoverage --",
        "prettier-format": "prettier --config .prettierrc.json 'src/**/*.ts' --write",
        "lint": "eslint . --ext .ts",
        "build": "npx tsc",
        "start": "node dist/index.js",
        "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
        "prisma:setup-dev": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed",
        "prisma:migrate": "prisma migrate dev --schema=src/infraestructure/prisma/schema.prisma",
        "prisma:generate": "prisma generate --schema=src/infraestructure/prisma/schema.prisma",
        "prisma:seed": "ts-node src/infraestructure/prisma/seed.ts",
        "prebuild": "npm run swagger",
        "swagger": "tsoa spec --json"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^4.3.1",
        "@types/uuid": "^8.3.4",
        "axios": "^0.27.2",
        "bcrypt": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "install": "^0.13.0",
        "npm": "^8.19.2",
        "prisma": "^4.3.1",
        "swagger-ui-express": "^4.5.0",
        "tsoa": "^4.1.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/node": "^18.7.14",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "concurrently": "^7.4.0",
        "eslint": "^8.23.1",
        "eslint-plugin-react": "^7.31.8",
        "jest": "^29.0.3",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "prisma": "^4.3.1",
        "ts-jest": "^29.0.0",
        "typescript": "^4.8.4"
    }
}
